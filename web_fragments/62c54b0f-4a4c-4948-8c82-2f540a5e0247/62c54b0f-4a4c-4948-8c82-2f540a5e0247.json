      "type" : "json",
      "value" : "[\r\n    {\r\n        \"id\":1,\r\n        \"nombre\":\"Cédula\",\r\n        \"Codigo\":\"CC\"\r\n    },\r\n    {\r\n        \"id\":2,\r\n        \"nombre\":\"Cedula de Extranjería\",\r\n        \"Codigo\":\"CE\"\r\n    },\r\n\t{\r\n        \"id\":3,\r\n        \"nombre\":\"Libreta Militar\",\r\n        \"Codigo\":\"LM\"\r\n    },\r\n\t{\r\n        \"id\":4,\r\n        \"nombre\":\"NIT\",\r\n        \"Codigo\":\"NIT\"\r\n    },\r\n\t{\r\n        \"id\":5,\r\n        \"nombre\":\"Nro Único de Identific Persona\",\r\n        \"Codigo\":\"NUIP\"\r\n    },\r\n    {\r\n        \"id\":6,\r\n        \"nombre\":\"Pasaporte\",\r\n        \"Codigo\":\"PA\"\r\n    },\r\n\t{\r\n        \"id\":7,\r\n        \"nombre\":\"Sin Especificar\",\r\n        \"Codigo\":\"SE\"\r\n    },\r\n\t{\r\n        \"id\":8,\r\n        \"nombre\":\"Tarjeta de Identidad\",\r\n        \"Codigo\":\"TI\"\r\n    }\r\n\t\r\n]\r\n        \r\n",
