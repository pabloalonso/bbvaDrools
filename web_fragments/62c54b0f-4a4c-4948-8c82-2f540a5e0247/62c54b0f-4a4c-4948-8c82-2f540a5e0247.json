    "precargaDatos" : {
      "type" : "expression",
      "value" : "var res=[]; \nvar form = {};\n\nform.fechaSolicitud = $data.formInput.notificaciones.fechaSolicitud;\nfor(var i=0; i <= $data.ConsultaTipoId.length;i++){\n    console.log($data.ConsultaTipoId[i].id);\n    if($data.ConsultaTipoId[i].id===$data.formInput.notificaciones.tipoIdentificacion){\n        form.tipoIdentificacion = $data.ConsultaTipoId[i].nombre;\n        break;\n    }\n}\nform.tipoVinculacion = $data.formInput.notificaciones.tipoVinculacion;\nfor(var i=0; i <= $data.consultaVinculacion.length;i++){\n    if($data.consultaVinculacion[i].id===$data.formInput.notificaciones.tipoIdentificacion){\n        form.tipoVinculacion = $data.consultaVinculacion[i].nombre;\n        break;\n    }\n}\nform.numeroIdentificacion = $data.formInput.notificaciones.numeroIdentificacion;\nform.primernombre = $data.formInput.notificaciones.primernombre;\nform.segundonombre = $data.formInput.notificaciones.segundonombre;\nform.primerapellido = $data.formInput.notificaciones.primerapellido;\nform.segundoapellido = $data.formInput.notificaciones.segundoapellido;\nform.correoElectronico = $data.formInput.notificaciones.correoElectronico;\nform.programa = $data.formInput.notificaciones.programa;\nform.cargo = $data.formInput.notificaciones.cargo;\nform.departamento = $data.formInput.notificaciones.departamento;\nform.fechaExamen = $data.formInput.notificaciones.fechaExamen;\nform.resultado = $data.formInput.notificaciones.resultado;\nform.observacion = $data.formInput.notificaciones.observacion;\nform.extranjero = $data.formInput.notificaciones.extranjero;\n\nform.departamentoDireccion = $data.formInput.notificaciones.departamentoDireccion;\nform.pais = $data.formInput.notificaciones.pais;\nform.lugarNacimiento = $data.formInput.notificaciones.lugarNacimiento;\nform.nacimiento = $data.formInput.notificaciones.nacimiento;\nform.direccion = $data.formInput.notificaciones.direccion;\nform.celular = $data.formInput.notificaciones.celular;\nform.telefono = $data.formInput.notificaciones.telefono;\nform.sexo = $data.formInput.notificaciones.sexo;\nform.estadoCivil = $data.formInput.notificaciones.estadoCivil;\n\nform.nivelEstudio = $data.formInput.notificaciones.nivelEstudio;\n\nvar fecha1=form.fechaSolicitud;\n\n\nfecha1 =fecha1.split(\"T\",1);\n\nif(form.fechaExamen){\nvar parts =form.fechaExamen.toISOString(); //.split(\"T\",1);\nvar fecha2 = parts.split(\"T\",1);\nform.fechaExamen = fecha2[0];\n\n}\nform.fechaSolicitud = fecha1[0];\n\nres.push(form);\n\nreturn res;",
      "exposed" : false
    },
